{
  "name": "ebnis",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "env-cmd -e dev gatsby develop --port 3022",
    "start:e2e": "env-cmd -e e2e gatsby develop --port 3022",
    "build": "gatsby build",
    "build:prod": "yarn remove-folders && env-cmd -e prod yarn build",
    "build:e2e": "yarn env-cmd -e e2e yarn build",
    "build:staging": "yarn env-cmd -e staging yarn build",
    "build-serve:e2e": "yarn build:e2e && yarn serve-local",
    "build-serve:staging": "yarn build:staging && yarn serve-local",
    "test:watch": "env-cmd -e test yarn jest --watch --runInBand",
    "test:coverage": "rimraf coverage && env-cmd -e test yarn jest --coverage --runInBand",
    "remove-folders": "rimraf public && rimraf .cache",
    "gen-gatsby-types": "apollo codegen:generate --endpoint=http://localhost:3022/___graphql --tagName=graphql --target=typescript --includes=src/components/**/*.ts* --outputFlat=src/graphql/gatsby-types",
    "gen-apollo-types": "rimraf src/graphql/apollo-types && apollo codegen:generate --endpoint=http://localhost:4022 --tagName=gql --target=typescript --outputFlat=src/graphql/apollo-types --includes=src/graphql/*.ts",
    "serve-local": "gatsby serve -p 3022",
    "build-deploy": "yarn build:prod && yarn deploy",
    "deploy": "netlify deploy -p -d public",
    "cy-open": "env-cmd -e e2e cypress open",
    "cy-run-task": "env-cmd -e e2e cypress run",
    "cy-run": "start-server-and-test serve-local :3022 cy-run-task",
    "build-cy-run": "start-server-and-test build-serve:e2e :3022 cy-run-task",
    "type-check": "yarn tsc --noEmit --project .",
    "type-check-cypress": "yarn tsc --noEmit --project ./cypress"
  },
  "dependencies": {
    "@absinthe/socket": "0.2.1",
    "@absinthe/socket-apollo-link": "0.2.1",
    "apollo-boost": "0.3.1",
    "apollo-cache-persist": "0.1.1",
    "classnames": "2.2.6",
    "date-fns": "1.29.0",
    "formik": "1.5.4",
    "gatsby": "2.4.2",
    "graphql": "14.2.1",
    "graphql-tag": "2.10.1",
    "immer": "3.1.2",
    "lodash": "4.17.13",
    "lodash.template": "4.5.0",
    "phoenix": "1.4.3",
    "react": "16.9.0-alpha.0",
    "react-apollo": "2.5.4",
    "react-dom": "16.9.0-alpha.0",
    "react-helmet-async": "1.0.2",
    "react-loadable": "5.5.0",
    "react-transition-group": "4.1.1",
    "semantic-ui-less": "2.4.1",
    "semantic-ui-react": "0.87.2",
    "yup": "0.27.0",
    "zen-observable-ts": "0.8.18"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.3.3",
    "@cypress/webpack-preprocessor": "4.0.3",
    "@types/classnames": "2.2.7",
    "@types/graphql": "14.2.0",
    "@types/jest": "24.0.12",
    "@types/lodash-es": "4.17.3",
    "@types/node": "12.0.0",
    "@types/phoenix": "1.4.0",
    "@types/reach__router": "1.2.4",
    "@types/react": "16.8.17",
    "@types/react-dom": "16.8.4",
    "@types/react-helmet": "5.0.8",
    "@types/react-loadable": "5.5.1",
    "@types/react-transition-group": "2.9.2",
    "@types/yup": "0.26.13",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "@typescript-eslint/parser": "1.11.0",
    "apollo": "2.11.0",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "babel-preset-gatsby": "0.1.11",
    "babel-preset-react-app": "9.0.0",
    "cheerio": "1.0.0-rc.3",
    "cypress": "3.4.0",
    "env-cmd": "9.0.1",
    "eslint": "6.0.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-graphql": "3.0.3",
    "eslint-plugin-react": "7.14.2",
    "eslint-plugin-react-hooks": "1.6.1",
    "gatsby-image": "2.0.41",
    "gatsby-plugin-alias-imports": "1.0.5",
    "gatsby-plugin-create-client-paths": "2.0.5",
    "gatsby-plugin-env-variables": "1.0.1",
    "gatsby-plugin-less": "2.0.12",
    "gatsby-plugin-manifest": "2.1.1",
    "gatsby-plugin-netlify": "2.0.16",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-sass": "2.0.11",
    "gatsby-plugin-sharp": "2.0.36",
    "gatsby-plugin-typescript": "2.0.13",
    "gatsby-source-filesystem": "2.0.33",
    "gatsby-transformer-sharp": "2.1.19",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.8.0",
    "jest-dom": "3.1.3",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-resolve": "24.8.0",
    "jest-watch-typeahead": "0.3.1",
    "less": "3.9.0",
    "node-sass": "4.11.0",
    "prettier": "1.18.2",
    "react-app-polyfill": "1.0.1",
    "react-testing-library": "6.1.2",
    "rimraf": "2.6.3",
    "serve": "11.0.0",
    "start-server-and-test": "1.9.0",
    "tslint": "5.16.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "4.0.0",
    "typescript": "3.5.2",
    "workbox-window": "4.3.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "resolutions": {
    "graphql": "14.2.1"
  }
}
